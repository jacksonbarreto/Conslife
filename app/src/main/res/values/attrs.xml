<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ButtonConslife">
        <attr name="text" format="string" />
        <attr name="type" format="enum">
            <enum name="primary_filled" value="1" />
            <enum name="primary_not_filled" value="2" />
            <enum name="secondary_filled" value="3" />
            <enum name="secondary_not_filled" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FloatingRoundCounter">
        <attr name="floating_round_counter_title" format="string" />
        <attr name="floating_round_counter_total" format="string" />
        <attr name="floating_round_counter_type" format="enum">
            <enum name="primary" value="1" />
            <enum name="secondary" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FloatingPoints">
        <attr name="floating_points_total" format="string" />
        <attr name="floating_points_type" format="enum">
            <enum name="primary" value="1" />
            <enum name="secondary" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SearchViewConslife">
        <attr name="search_view_conslife_hint" format="string" />
    </declare-styleable>

    <declare-styleable name="DoubleHeading">
        <attr name="double_heading_title" format="string" />
        <attr name="double_heading_sub_title" format="string" />
        <attr name="double_heading_size" format="enum">
            <enum name="h1" value="1" />
            <enum name="h2" value="2" />
            <enum name="h3" value="3" />
            <enum name="h4" value="4" />
            <enum name="h5" value="5" />
            <enum name="h6" value="6" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MissionCard">
        <attr name="mission_card_title" format="string" />
        <attr name="mission_card_deadline" format="string" />
        <attr name="mission_card_date" format="string" />
        <attr name="mission_card_vacancies" format="string" />
        <attr name="mission_card_points" format="string" />
        <attr name="mission_card_image_src" format="string" />
    </declare-styleable>

    <declare-styleable name="RewardCard">
        <attr name="reward_card_title" format="string" />
        <attr name="reward_card_deadline" format="string" />
        <attr name="reward_card_points" format="string" />
        <attr name="reward_card_available" format="string" />
        <attr name="reward_card_image_src" format="string" />
        <attr name="reward_card_description" format="string" />
    </declare-styleable>

</resources>