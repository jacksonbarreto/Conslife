<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ButtonConslife">
        <attr name="text" format="string" />
        <attr name="type" format="enum">
            <enum name="primary_filled" value="1" />
            <enum name="primary_not_filled" value="2" />
            <enum name="secondary_filled" value="3" />
            <enum name="secondary_not_filled" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FloatingRoundCounter">
        <attr name="floating_round_counter_title" format="string" />
        <attr name="floating_round_counter_total" format="string" />
        <attr name="floating_round_counter_type" format="enum">
            <enum name="primary" value="1" />
            <enum name="secondary" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FloatingPoints">
        <attr name="floating_points_total" format="string" />
        <attr name="floating_points_type" format="enum">
            <enum name="primary" value="1" />
            <enum name="secondary" value="2" />
        </attr>
    </declare-styleable>


    <declare-styleable name="TwoLines">
        <attr name="line_one_text" format="string" />
        <attr name="line_one_font_size" format="float" />
        <attr name="line_one_text_style" format="string" />
        <attr name="line_one_text_color" format="integer" />

        <attr name="line_two_text" format="string" />
        <attr name="line_two_font_size" format="float" />
        <attr name="line_two_text_style" format="string" />
        <attr name="line_two_text_color" format="integer" />
    </declare-styleable>

</resources>